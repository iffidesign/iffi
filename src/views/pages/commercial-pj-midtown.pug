extends ../layout/main

block VARIABLES
  - var pageTitle = 'Commercial PJ Midtown Sales Gallery';
  - var name = 'pj-midtown';

block BODY
  .key-visual
    picture
      source(media="(max-width:991px)" srcset="../../images/commercial-pj-midtown-mobile.jpg")
      img.img-fluid(src="../../images/commercial-pj-midtown.jpg", alt="")
    .product__info
      .product__category Commercial
      .product__title PJ Midtown Sales Gallery

  .divider

  .Album
    .Album__content
      .Album__image: img(src="../../images/PJ_Midtown_Sales_Gallery/01.jpg")
      .Album__image: img(src="../../images/PJ_Midtown_Sales_Gallery/02.jpg")
      .Album__image: img(src="../../images/PJ_Midtown_Sales_Gallery/03.jpg")
      .Album__image: img(src="../../images/PJ_Midtown_Sales_Gallery/04.jpg")
